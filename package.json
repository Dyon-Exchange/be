    "start": "echo \"This is for PRODUCTION ONLY, use 'npm run dev' for development server'\" && ENV=PROD node ./dist/src/bin/www.js"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
