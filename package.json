    "@typegoose/typegoose": "^7.6.0",
    "bcrypt": "^5.0.1",
    "debug": "^4.3.1",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-joi-router": "^8.0.0",
    "koa-passport": "^4.1.4",
    "koa-router": "^10.0.0",
    "mongoose": "5.10.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.6.0"
  },
  "devDependencies": {
