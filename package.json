{
  "name": "be",
  "version": "1.0.0",
  "description": "WineBit backend Koa.js server",
  "main": "dist/bin/www.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "rm -rf ./dist/",
    "compile": "tsc && DEBUG=* node ./dist/bin/www.js",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile\"",
